local player = game.Players.LocalPlayer
local character = player.Character
local closestEnemy = nil
for _, enemy in pairs(workspace:GetChildren()) do 
	if enemy:IsA("Model") and enemy ~= character then
		if not closestEnemy then
			closestEnemy = enemy
		else
			if (enemy.PrimaryPart.Position - character.PrimaryPart.Position).magnitude < (closestEnemy.PrimaryPart.Position - character.PrimaryPart.Position).magnitude then
				closestEnemy = enemy
			end
		end
	end
end

character:MoveTo(closestEnemy.PrimaryPart.Position)

character:WaitForChild("Humanoid"):Attack(closestEnemy)
